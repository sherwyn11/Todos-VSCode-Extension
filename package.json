{
  "name": "todos-vscode-extension",
  "displayName": "Todos-VSCode-Extension",
  "description": "Manage your Todos easily right in VSCode itself!",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:todos-vscode-extension.helloWorld",
    "onCommand:todos-vscode-extension.refresh"
  ],
  "main": "./build/ext-src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "todos-vscode-extension.helloWorld",
        "category": "Todos-VSCode",
        "title": "Hello World"
      },
      {
        "command": "todos-vscode-extension.refresh",
        "category": "Todos-VSCode",
        "title": "Refresh"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "build": "./scripts/build-non-split.js && tsc -p tsconfig.extension.json",
    "watch": "chokidar \"**/*.ts\" \"**/*.tsx\" \"**/*.json\" -c \"yarn run build\" -i \"node_modules\" -i \"build\""
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/vscode": "^1.55.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "chokidar": "^3.5.1",
    "chokidar-cli": "^2.1.0",
    "concurrently": "^6.0.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "react-scripts": "^4.0.3",
    "rewire": "^5.0.0",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "terser": "^5.6.1"
  }
}
